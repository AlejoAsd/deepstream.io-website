<ul class="tree-nav subsection">
	{{#each this}}
		<li class="{{ifactive 'level2' 'open' @root @key}}">
			<div class="sub-section-toggle">
				<span cplass="icon ion-chevron-right"></span>
				<span class="title">{{@key}}</span>
			</div>
			<ul class="entry">
				{{#each this}}
					<li class="{{ifactive 'level3' 'active' @root slug}}">
						<a href="{{path}}">{{title}}</a>
					</li>
				{{/each}}
			</ul>
		</li>
	{{/each}}
</ul>