<link rel="stylesheet" type="text/css" href="{{asset 'css/docs.css'}}" />

<div class="two-col docs">
	<div class="col left small subnav">
		{{> "treenav" }}
	</div>
	<div class="col big right">
		<div class="header"
		{{#if image}}
			style="background-image:url({{image}})"
		{{/if}}>
			{{> "breadcrumbs"}}
			<h1>{{title}}</h1>
		</div>
		<div class="content">
			{{{contents}}}
		</div>
	</div>
</div>